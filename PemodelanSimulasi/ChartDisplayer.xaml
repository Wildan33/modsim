<Window x:Class="PemodelanSimulasi.ChartDisplayer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PemodelanSimulasi"
        xmlns:d3="http://research.microsoft.com/DynamicDataDisplay/1.0"
        mc:Ignorable="d"
        Title="ChartDisplayer" Height="450" Width="800" >
    <Grid>
        <d3:ChartPlotter x:Name="plotter" Margin="10,36,16.6,27"  >
            <d3:HorizontalAxisTitle Content="Years"/>
            <d3:VerticalAxisTitle Content="Model"/>

        </d3:ChartPlotter>
    </Grid>
</Window>
